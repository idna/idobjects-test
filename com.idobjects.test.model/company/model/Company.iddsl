package com.idobjects.test.company{
	entity Employee{
		String firstName;
		String lastName;
		int age;
		Department department;
	}
	
	entity Freelancer extends Employee{
		DateTime startOfWork;
		DateTime endOfWork;
	}
	
	entity Department {
		String name;
		List[Employee] members [inverse=department];
		Employee boss;
		int size;
	}
}

